<h1>Listing user_assessments</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_assessments.each do |user_assessment| %>
      <tr>
        <td><%= user_assessment.user_id %></td>
        <td><%= link_to 'Show', user_assessment %></td>
        <td><%= link_to 'Edit', edit_user_assessment_path(user_assessment) %></td>
        <td><%= link_to 'Destroy', user_assessment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

